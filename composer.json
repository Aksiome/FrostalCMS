{
    "name": "aksiome/frostal",
    "description": "Frostal is a content first CMS",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Frostal\\": "src"
        }
    },
    "require": {
        "psr/http-server-middleware": "^1.0",
        "psr/http-factory": "^1.0",
        "nyholm/psr7": "^1.2",
        "nyholm/psr7-server": "^0.4.1",
        "symfony/var-exporter": "^5.0",
        "http-interop/response-sender": "^1.0",
        "latte/latte": "^2.6",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/var-dumper": "^5.0",
        "squizlabs/php_codesniffer": "^3.5",
        "php-coveralls/php-coveralls": "^2.2"
    }
}
